{
  "name": "preload.io",
  "version": "1.0.0",
  "description": "Asset preloader",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "npm run build",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch",
    "preexample": "npm run build",
    "example": "browserify examples/load.js > examples/build.js",
    "pretest": "npm run build",
    "test": "zuul --open --local 8080 -- spec/*.js",
    "prezuul": "npm run build",
    "zuul": "zuul -- spec/*.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "Matt Styles",
  "repository": "git://github.com/mattstyles/preloadio.git",
  "license": "ISC",
  "dependencies": {
    "eventemitter3": "^1.1.1",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "^4.1.1",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "eslint": "^1.3.1",
    "tape": "^4.2.0",
    "zuul": "^3.6.0"
  }
}
